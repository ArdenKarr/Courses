        for (int )
    }
    return snape;
}

int main()
{
    int n;
    cin >> n;
    int **snape = new int*[n];
    for (int i = 0; i < n; i++)
        snape[i] = new int[n];
    snape = snaping(n);
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < n; j++)
            cout << snape[i][j];
        cout << endl;
    }
    for (int i = 0; i < n; i++)
        delete [] snape[i];
    return 0;
}
